<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  主题色  -->
    <Color x:Key="AcArticleAccentColor">#FFFD4C5D</Color>
    <Color x:Key="AcArticleAccentDarkColor">#FFBE5A64</Color>
    <Color x:Key="AcArticleAccentDarkDarkColor">#FFA41926</Color>
    <Color x:Key="AcArticleAccentLightColor">#FFFE7985</Color>
    <Color x:Key="AcArticleAccentLightLightColor">#FFFE9CA6</Color>
    <!--  文本颜色  -->
    <Color x:Key="AcArticlePrimaryTextDarkColor">#DE000000</Color>
    <Color x:Key="AcArticleSecondaryTextDarkColor">#8A000000</Color>
    <Color x:Key="AcArticleTertiaryTextDarkColor">#61000000</Color>
    <Color x:Key="AcArticlePrimaryTextLightColor">#FFFFFFFF</Color>
    <Color x:Key="AcArticleSecondaryTextLightColor">#B3FFFFFF</Color>
    <Color x:Key="AcArticleTertiaryTextLightColor">#80FFFFFF</Color>
    <!--  阴影颜色  -->
    <Color x:Key="AcArticleShadowColor">#AA000000</Color>
</ResourceDictionary>